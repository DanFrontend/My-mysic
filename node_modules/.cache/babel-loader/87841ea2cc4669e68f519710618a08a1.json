,CAApB;AAGD;AAVQ;AAzBgB,CAAf,CAAd;AAuCA,eAAeR,KAAf","sourcesContent":["import Vue from 'vue';\nimport Vuex from 'vuex';\n\nVue.use(Vuex);\n\nconst store = new Vuex.Store({\n  namespaced: true,\n  state: {\n    songs: [\n      {\n        executor: \"Виктор Цой\",\n        song: \"Печаль\",\n        src: \"/music/pech.mp3\",\n        filterExecutor: 'tcoy'\n      },\n      {\n        executor: \"Ноль\",\n        song: \"Кислотный дождь\",\n        src: \"/music/kislot.mp3\",\n        filterExecutor: 'nol'\n      },\n      {\n        executor: \"ГО\",\n        song: \"Мы идем в тишине\",\n        src: \"/music/tish.mp3\",\n        filterExecutor: 'go'\n      }\n    ],\n    songsFilter: []\n  },\n  mutations: {\n    filterSongs(state, value) {\n      if(state.songsFilter.length === 0) {\n        state.songsFilter = state.songs;\n        // return\n      }\n\n      state.songsFilter = state.songs.filter(s => {\n        return value.indexOf(s.filterExecutor) !== -1\n      })\n    },\n  }\n})\n\nexport default store;"]}]}