;AAEA,MAAA,QAAA,CAAA,gBAAA,CAAA,OAAA,EAAA,OAAA,CAAA,UAAA,KAAA,EAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,IAAA;AACA,OAFA;AAGA,KA7BA;AA8BA,IAAA,SA9BA,uBA8BA;AACA,WAAA,IAAA,GAAA,EAAA;AACA,WAAA,IAAA,GAAA,EAAA;AACA,WAAA,GAAA,GAAA,EAAA;AAEA,WAAA,SAAA,CAAA,CAAA,KAAA,IAAA,EAAA,KAAA,GAAA,EAAA,KAAA,IAAA,CAAA;AAEA,MAAA,QAAA,CAAA,gBAAA,CAAA,OAAA,EAAA,OAAA,CAAA,UAAA,KAAA,EAAA;AACA,QAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,OAFA;AAGA,KAxCA;AAyCA,IAAA,cAzCA,0BAyCA,GAzCA,EAyCA;AAAA;;AACA,UAAA,SAAA,GAAA,QAAA,CAAA,gBAAA,CAAA,GAAA,CAAA;AACA,UAAA,OAAA,GAAA,KAAA,CAAA,KAAA,CAAA,IAAA,EAAA,SAAA,CAAA;AAEA,MAAA,OAAA,CAAA,IAAA,CAAA,UAAA,EAAA;AAAA,eAAA,EAAA,CAAA,OAAA,KAAA,IAAA;AAAA,OAAA,IACA,OAAA,CAAA,OAAA,CAAA,UAAA,KAAA,EAAA;AACA,QAAA,GAAA,KAAA,aAAA,GAAA,KAAA,CAAA,IAAA,GAAA,EAAA,GACA,GAAA,KAAA,aAAA,GAAA,KAAA,CAAA,IAAA,GAAA,EAAA,GACA,GAAA,KAAA,YAAA,GAAA,KAAA,CAAA,GAAA,GAAA,EAAA,GAAA,IAFA;;AAIA,QAAA,KAAA,CAAA,SAAA,CAAA,CAAA,KAAA,CAAA,IAAA,EAAA,KAAA,CAAA,GAAA,EAAA,KAAA,CAAA,IAAA,CAAA;;AAEA,QAAA,KAAA,CAAA,OAAA,GAAA,KAAA;AACA,OARA,CADA,GAUA,OAAA,CAAA,OAAA,CAAA,UAAA,KAAA,EAAA;AACA,QAAA,GAAA,KAAA,aAAA,GAAA,KAAA,CAAA,IAAA,GAAA,CAAA,MAAA,EAAA,KAAA,EAAA,IAAA,EAAA,UAAA,EAAA,QAAA,EAAA,QAAA,EAAA,OAAA,EAAA,OAAA,EAAA,OAAA,EAAA,OAAA,EAAA,KAAA,CAAA,GACA,GAAA,KAAA,aAAA,GAAA,KAAA,CAAA,IAAA,GAAA,CAAA,KAAA,EAAA,IAAA,EAAA,IAAA,CAAA,GACA,GAAA,KAAA,YAAA,GAAA,KAAA,CAAA,GAAA,GAAA,CAAA,OAAA,EAAA,KAAA,EAAA,SAAA,EAAA,OAAA,CAAA,GAAA,IAFA;;AAIA,QAAA,KAAA,CAAA,SAAA,CAAA,CAAA,KAAA,CAAA,IAAA,EAAA,KAAA,CAAA,GAAA,EAAA,KAAA,CAAA,IAAA,CAAA;;AAEA,QAAA,KAAA,CAAA,OAAA,GAAA,IAAA;AACA,OARA,CAVA;AAmBA;AAhEA;AAnCA,CAAA","sourcesContent":["<template>\n  <div class=\"sidebar\">\n    <div class=\"sidebar-setting\">\n      <button @click=\"chooseAll()\">Выбрать всё</button>\n      <button @click=\"removeAll()\">Убрать всё</button>\n    </div>\n    <div class=\"sidebar-item\">\n      <div class=\"sidebar-title\" @click=\"toggleCategory('[data-exec]')\">Исполнители</div>\n      <label v-for=\"(executor, i) in dataSidebar.executor\" :key=\"i\">\n        <input type=\"checkbox\" :data-exec=\"executor.exec\" checked=\"checked\" @click=\"isFilter($event)\" />\n        <span>{{executor.name}}</span>\n      </label>\n    </div>\n    <div class=\"sidebar-item\">\n      <div class=\"sidebar-title\" @click=\"toggleCategory('[data-lang]')\">Язык</div>\n      <label v-for=\"(language, i) in dataSidebar.language\" :key=\"i\">\n        <input type=\"checkbox\" :data-lang=\"language.lang\" checked=\"checked\" @click=\"isFilter($event)\" />\n        <span>{{language.name}}</span>\n      </label>\n    </div>\n    <div class=\"sidebar-item\">\n      <div class=\"sidebar-title\" @click=\"toggleCategory('[data-moo]')\">Настроение</div>\n      <label v-for=\"(mood, i) in dataSidebar.mood\" :key=\"i\">\n        <input type=\"checkbox\" :data-moo=\"mood.moo\" checked=\"checked\" @click=\"isFilter($event)\" />\n        <span>{{mood.name}}</span>\n      </label>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapMutations } from 'vuex';\n\nexport default {\n  name: 'Sidebar',\n  data() {\n    return {\n      dataSidebar: {\n        executor: [\n          {name: 'Цой', exec: 'tcoy'},\n          {name: 'Ноль', exec: 'nol'},\n          {name: 'ГО', exec: 'go'},\n          {name: 'Скрябин', exec: 'skryabin'},\n          {name: 'Лявон Вольскі', exec: 'lyavon'},\n          {name: 'Ляпис Трубецкой', exec: 'lyapis'},\n          {name: 'Баста', exec: 'basta'},\n          {name: 'Сразу Май', exec: 'srazu'},\n          {name: 'Порнофильмы', exec: 'filmy'},\n          {name: 'Король и шут', exec: 'korol'},\n          {name: 'Тік', exec: 'tik'}\n        ],\n        language: [\n          {name: 'Русский', lang: 'rus'},\n          {name: 'Белорусский', lang: 'by'},\n          {name: 'Украинский', lang: 'ua'}\n        ],\n        mood: [\n          {name: 'Весёлые', moo: 'funny'},\n          {name: 'Грустные', moo: 'sad'},\n          {name: 'Классические', moo: 'classic'},\n          {name: 'Тяжелые', moo: 'heavy'}\n        ]\n      },\n      exec: ['tcoy', 'nol', 'go', 'skryabin', 'lyavon', 'lyapis', 'basta', 'srazu', 'filmy', 'korol', 'tik'],\n      lang: ['rus', 'by', 'ua'],\n      moo: ['funny', 'sad', 'classic', 'heavy']\n    }\n  },\n  methods: {\n    ...mapMutations({\n      setFilter: 'setFilter'\n    }),\n    isFilter(v) {\n      this.exec.indexOf(v.target.dataset.exec) != -1 ?\n      this.exec.remove(v.target.dataset.exec) :\n      this.exec.push(v.target.dataset.exec)\n      \n      this.moo.indexOf(v.target.dataset.moo) != -1 ?\n      this.moo.remove(v.target.dataset.moo) :\n      this.moo.push(v.target.dataset.moo)\n      \n      this.lang.indexOf(v.target.dataset.lang) != -1 ?\n      this.lang.remove(v.target.dataset.lang) :\n      this.lang.push(v.target.dataset.lang) \n\n      this.setFilter([this.exec, this.moo, this.lang]);\n    },\n    chooseAll() {\n      this.exec = ['tcoy', 'nol', 'go', 'skryabin', 'lyavon', 'lyapis', 'basta', 'srazu', 'filmy', 'korol', 'tik'];\n      this.lang = ['rus', 'by', 'ua'];\n      this.moo = ['funny', 'sad', 'classic', 'heavy'];\n\n      this.setFilter([this.exec, this.moo, this.lang]);\n\n      document.querySelectorAll('input').forEach(input => {\n        input.checked = true\n      })\n    },\n    removeAll() {\n      this.exec = [];\n      this.lang = [];\n      this.moo = [];\n\n      this.setFilter([this.exec, this.moo, this.lang]);\n\n      document.querySelectorAll('input').forEach(input => {\n        input.checked = false\n      })\n    },\n    toggleCategory(val) {\n      let dataCatNL = document.querySelectorAll(val);\n      let dataCat = Array.apply(null, dataCatNL);\n      \n      dataCat.some((el) => el.checked === true) ?\n      dataCat.forEach(input => {\n        val === '[data-exec]' ? this.exec = [] : \n        val === '[data-lang]' ? this.lang = [] : \n        val === '[data-moo]' ? this.moo = [] : null;\n\n        this.setFilter([this.exec, this.moo, this.lang]);\n\n        input.checked = false\n      }) :\n      dataCat.forEach(input => {\n        val === '[data-exec]' ? this.exec = ['tcoy', 'nol', 'go', 'skryabin', 'lyavon', 'lyapis', 'basta', 'srazu', 'filmy', 'korol', 'tik'] : \n        val === '[data-lang]' ? this.lang = ['rus', 'by', 'ua'] : \n        val === '[data-moo]' ? this.moo = ['funny', 'sad', 'classic', 'heavy'] : null;\n\n        this.setFilter([this.exec, this.moo, this.lang]);\n\n        input.checked = true\n      });\n    }\n  }\n}\n</script>\n\n<style scoped lang=\"scss\">\n  .sidebar {\n    flex-shrink: 0;\n    margin-top: 10px;\n    border-radius: 10px;\n    padding: 20px;\n    width: 280px;\n    height: 100%;\n    background: #ebf5fc;\n    box-shadow: -2px -2px 5px rgba(255, 255, 255, 1),\n              3px 3px 5px rgba(0, 0, 0, 0.1);\n\n    &-title {\n      text-align: left;\n      color: #61677c;\n      text-shadow: 1px 1px 1px #fff;\n      cursor: pointer;\n      user-select: none; \n    }\n\n    &-setting {\n      margin-bottom: 5px;\n      display: flex;\n      justify-content: space-between;\n\n      & button {\n        padding: 5px 8px;\n        border-radius: 15px;\n        background-color: transparent;\n        color: #61677c;\n        font-size: 13px;\n        box-shadow: 2px 2px 5px #c8c8c8, -2px -2px 5px white;\n        transition: box-shadow .2s;\n        border: 0;\n        outline: 0;\n        cursor: pointer;\n        &:hover {\n        box-shadow:\n            8px 8px 10px #c8c8c8,\n            -8px -8px 10px white;\n        }\n        &:active {\n            box-shadow:\n                2px 2px 2px #c8c8c8,\n                -2px -2px 2px white;\n        }\n      }\n    }\n    &-item {\n        margin: 17px 0;\n\n      & label {\n        display: flex;\n        cursor: pointer;\n        margin: 5px 0;\n        position: relative;\n      }\n      & input {\n        appearance: none;\n        &::before {\n          position: absolute;\n          top: 1px;\n          border-radius: 50%;\n          content: \"\";\n          line-height: 0;\n          width: 16px;\n          height: 16px;\n          margin: 0 8px;\n          font-size: 16px;\n          box-shadow: -5px -5px 10px #fff, 5px 5px 10px #babecc;\n          transition: all 0.2s ease-in-out;\n        }\n        &:checked::before {\n          background-color: #00ff00bf\n        }\n        &:disabled {\n          & + span {\n            color: #9fa1a2\n          }\n        }\n      }\n      & span {\n        padding-left: 26px;\n        font-size: 14px;\n        text-shadow: 1px 1px 1px #fff;\n      }\n    }\n  }\n</style>"],"sourceRoot":"src/components"}]}